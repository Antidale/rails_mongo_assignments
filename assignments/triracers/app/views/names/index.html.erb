<p id="notice"><%= notice %></p>

<h1>Listing Names</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>City</th>
      <th>State</th>
      <th>Swim distance</th>
      <th>Swim units</th>
      <th>Bike distance</th>
      <th>Bike units</th>
      <th>Run distance</th>
      <th>Run units</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @names.each do |name| %>
      <tr>
        <td><%= name.date %></td>
        <td><%= name.city %></td>
        <td><%= name.state %></td>
        <td><%= name.swim_distance %></td>
        <td><%= name.swim_units %></td>
        <td><%= name.bike_distance %></td>
        <td><%= name.bike_units %></td>
        <td><%= name.run_distance %></td>
        <td><%= name.run_units %></td>
        <td><%= link_to 'Show', name %></td>
        <td><%= link_to 'Edit', edit_name_path(name) %></td>
        <td><%= link_to 'Destroy', name, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Name', new_name_path %>
