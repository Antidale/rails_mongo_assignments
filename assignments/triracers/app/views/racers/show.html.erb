<p id="notice"><%= notice %></p>

<p>
  <strong>First name:</strong>
  <%= @racer.first_name %>
</p>

<p>
  <strong>Last name:</strong>
  <%= @racer.last_name %>
</p>

<p>
  <strong>Gender:</strong>
  <%= @racer.gender %>
</p>

<p>
  <strong>Birth year:</strong>
  <%= @racer.birth_year %>
</p>

<p>
  <strong>City:</strong>
  <%= @racer.city %>
</p>

<p>
  <strong>State:</strong>
  <%= @racer.state %>
</p>

<%= link_to 'Edit', edit_racer_path(@racer) %> |
<%= link_to 'Back', racers_path %>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Date</th>
      <th>Overall</th>
      <th>Gender</th>
      <th>Group</th>
      <th>Results</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @races.each do |entry| %>
    <tr>
      <td><%= entry.race_name %></td>
      <td><%= entry.race_date %></td>
      <td><%= entry.overall.place   if entry.overall %></td>
      <td><%= entry.gender.place  if entry.gender %></td>
      <td><%= entry.group.place if entry.group %></td>
      <td><%= link_to "Results", race_path(entry.race)%></td>
    </tr>
    <% end %>



